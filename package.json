    "changeset:publish": "changeset publish",
    "release": "yarn changeset:version && yarn changeset:publish"
  },
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "nx": "18.3.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1"
  }
}
